<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>聂风</name>
    <description>
        NieFeng
    </description>
    <author email="assmdx183@163.com" href="http://github.com/assmdx">
        Yancloud Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="hdpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon density="xhdpi" src="res/icon/android/icon-36-ldpi.png" />
    <splash density="land-hdpi" src="res/screen/android/bg.jpg" />
    <splash density="land-ldpi" src="res/screen/android/bg.jpg" />
    <splash density="land-mdpi" src="res/screen/android/bg.jpg" />
    <splash density="land-xhdpi" src="res/screen/android/bg.jpg" />
    <splash density="port-hdpi" src="res/screen/android/bg.jpg" />
    <splash density="port-ldpi" src="res/screen/android/bg.jpg" />
    <splash density="port-mdpi" src="res/screen/android/bg.jpg" />
    <splash density="port-xhdpi" src="res/screen/android/bg.jpg" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="org.jeremyup.cordova.x5engine.X5WebViewEngine" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
